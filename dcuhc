#include <iostream>
using namespace std;

int main() {
    int number;
    int sum = 0;
    cin >> number;

    while (number != 0) {
        sum += number;
        cin >> number;
    }

    cout << sum << endl;

    return 0;
}


#include <iostream>
using namespace std;

int main() {
    int number;
    cin >> number;

    cout << endl;

    while (number > 0) {
        cout << "Отсчет " << number << endl;
        number--;
    }

    cout << endl;

    return 0;
}


#include <iostream>
using namespace std;

int main() {
    int number;
    int count = 0;
    int sum = 0;
    cin >> number;

    while (number != 0) {
        count++; 
        sum += number;
        cin >> number;
    }

    cout << "[ + ] Количество чисел " << count << endl;
    cout << "[ + ] Сумма " << sum << endl;

    return 0;
}


#include <iostream>
using namespace std;

int main() {
    int number;
    int count = 0;
    cin >> number;
    if (number == 0) {
        count = 1;
    }
    else {
        int temp = number;

        while (temp > 0) {
            count++;
            temp /= 10;    
        }
    }

    cout << count << endl;

    return 0;
}



#include <iostream>
using namespace std;

int main() {
    int number;
    cin >> number;

    int i = 1;

    while (i <= 10) {
        cout << "[ + ] " << number << " × " << i << " = " << number * i << endl;
        i++;
    }

    return 0;
}
    
